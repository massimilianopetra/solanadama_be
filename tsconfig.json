      "module": "commonjs",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "sourceMap": true,
      "outDir": "./build/",
      "skipLibCheck": true
    },
    "include": ["./src/**/*.ts"]
